{
  "name": "@eatsjobs/media-mock",
  "version": "0.4.1",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eatsjobs/media-mock.git"
  },
  "keywords": [
    "media",
    "mediaDevices",
    "getUserMedia",
    "mock",
    "browser",
    "testing",
    "playwright",
    "video",
    "mocking",
    "webcam",
    "automated-testing",
    "mock-media",
    "ui-testing"
  ],
  "license": "MIT",
  "umd:main": "./dist/media-mock.umd.cjs",
  "unpkg": "./dist/media-mock.umd.cjs",
  "author": "@eatsjobs Pasquale Mangialavori",
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist/**",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "main": "./dist/media-mock.umd.cjs",
  "module": "./dist/media-mock.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/main.d.ts",
        "default": "./dist/media-mock.js"
      },
      "require": {
        "types": "./dist/main.d.cts",
        "default": "./dist/media-mock.umd.cjs"
      }
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "test": "vitest",
    "test-coverage": "vitest --coverage",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "npm run build && npx changeset publish",
    "check-types": "npx attw $(npm pack) --profile=node16"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.9",
    "@codecov/vite-plugin": "^1.4.0",
    "@vitest/browser": "^2.1.5",
    "@vitest/coverage-v8": "^2.1.5",
    "playwright": "^1.48.2",
    "typescript": "~5.6.3",
    "vite": "^5.4.11",
    "vite-plugin-dts": "^4.5.0",
    "vitest": "^2.1.5",
    "@arethetypeswrong/cli": "~0.17.4"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=16.0.0"
  }
}
