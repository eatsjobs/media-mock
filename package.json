{
  "name": "@eatsjobs/media-mock",
  "version": "1.0.1",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eatsjobs/media-mock.git"
  },
  "keywords": [
    "media",
    "mediaDevices",
    "getUserMedia",
    "mock",
    "browser",
    "testing",
    "playwright",
    "video",
    "mocking",
    "webcam",
    "automated-testing",
    "mock-media",
    "ui-testing"
  ],
  "license": "MIT",
  "umd:main": "./dist/media-mock.umd.min.js",
  "unpkg": "./dist/media-mock.umd.min.js",
  "author": "@eatsjobs Pasquale Mangialavori",
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist/**",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "main": "./dist/media-mock.cjs",
  "module": "./dist/media-mock.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/main.d.ts",
        "default": "./dist/media-mock.js"
      },
      "require": {
        "types": "./dist/main.d.cts",
        "default": "./dist/media-mock.cjs"
      }
    }
  },
  "scripts": {
    "lint": "biome check",
    "lint:fix": "biome check --fix",
    "dev": "vite",
    "build": "tsc && vite build",
    "test": "vitest --run",
    "test-coverage": "vitest --run --coverage",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "npm run build && npx changeset publish",
    "check-types": "npx attw $(npm pack) --profile=node16"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "~0.18.2",
    "@biomejs/biome": "2.2.3",
    "@changesets/cli": "^2.29.6",
    "@codecov/vite-plugin": "^1.9.1",
    "@types/node": "^24.3.1",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-istanbul": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "playwright": "^1.55.0",
    "typescript": "~5.7.3",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=16.0.0"
  }
}
